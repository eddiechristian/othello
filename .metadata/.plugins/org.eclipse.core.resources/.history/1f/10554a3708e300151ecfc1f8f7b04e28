//use std::*;

enum BoardSquareState {
	EMPTY,
	BLACK,
	WHITE
}

#[derive(Debug)]
type ROW = Vec<BoardSquareState>;

struct OthelloBoard{
	rows: Vec<ROW>
}

fn main() {
	
	let n = 4;
	let size :usize = 2 * n;
	
	let row0 = vec!{BoardSquareState::EMPTY,BoardSquareState::EMPTY};
	let mut board = OthelloBoard{ rows: vec![] };
	
    println!("row0 = {:?}",row0);
    println!("size = {:?}",size);
        
}
